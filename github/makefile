# -*- makefile -*-
## -----------------------------------------------------------------------
## -----------------------------------------------------------------------

deps += lib/config.sh
deps += lib/prompt.sh

## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
all: $(deps)
	./myauth.sh 2>&1 | tee log


## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
lib/config.sh lib/prompt.sh:
	lib/gen_config.sh

## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
init:
	lib/gen_config.sh

## -----------------------------------------------------------------------
## -----------------------------------------------------------------------
clean:
	$(RM) -r data
	$(RM) lib/config.sh
	$(RM) lib/prompt.sh

sterile : clean

# [EOF]
